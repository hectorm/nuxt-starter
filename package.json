{
  "private": true,
  "name": "app",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "scripts": {
    "clean": "nuxi cleanup",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi start",
    "prepare": "nuxi prepare",
    "lint": "run-s lint:*",
    "lint:typecheck": "nuxi typecheck",
    "lint:eslint": "eslint --max-warnings 0 ./",
    "lint:prettier": "prettier --list-different ./",
    "lint:prisma": "prisma validate",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix ./",
    "format:prettier": "prettier --write ./",
    "format:zenstack": "zenstack format",
    "format:prisma": "prisma format",
    "db:generate": "run-s db:generate:zenstack db:generate:prisma",
    "db:generate:zenstack": "zenstack generate --offline",
    "db:generate:prisma": "prisma generate --no-hints",
    "db:baseline": "prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script > ./prisma/migrations/0_init/migration.sql",
    "db:reset": "run-s \"db:push --force-reset\" db:seed",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "docker:start": "docker compose up --remove-orphans --build --detach",
    "docker:stop": "docker compose down --remove-orphans",
    "docker:restart": "run-s docker:stop docker:start",
    "postinstall": "run-s prepare db:generate"
  },
  "zenstack": {
    "schema": "./prisma/schema.zmodel",
    "prisma": "./prisma/schema.prisma"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "tsx ./prisma/seed.ts"
  },
  "dependencies": {
    "@iconify-json/lucide": "~1.2.44",
    "@iconify-json/simple-icons": "~1.2.35",
    "@nuxt/devtools": "~2.4.1",
    "@nuxt/fonts": "~0.11.4",
    "@nuxt/icon": "~1.13.0",
    "@nuxt/image": "~1.10.0",
    "@nuxt/ui": "~3.1.2",
    "@nuxtjs/color-mode": "~3.5.2",
    "@nuxtjs/i18n": "~9.5.4",
    "@pinia/nuxt": "~0.11.0",
    "@prisma/client": "~6.8.2",
    "@prisma/engines": "~6.8.2",
    "@prisma/internals": "~6.8.2",
    "@prisma/migrate": "~6.8.2",
    "@trpc/client": "~11.1.2",
    "@trpc/server": "~11.1.2",
    "@vueuse/core": "~13.2.0",
    "@vueuse/nuxt": "~13.2.0",
    "@zenstackhq/runtime": "~2.15.0",
    "@zenstackhq/server": "~2.15.0",
    "h3": "~1.15.3",
    "jmespath": "~0.16.0",
    "jose": "~6.0.11",
    "nitropack": "~2.11.12",
    "nuxt": "~3.17.4",
    "oauth4webapi": "~3.5.1",
    "pinia": "~3.0.2",
    "pino": "~9.7.0",
    "pino-pretty": "~13.0.0",
    "prisma": "~6.8.2",
    "superjson": "~2.2.2",
    "tailwindcss": "~4.1.7",
    "trpc-nuxt": "~1.0.5",
    "vue": "~3.5.14",
    "vue-i18n": "~10.0.7",
    "vue-router": "~4.5.1",
    "zenstack": "~2.15.0",
    "zod": "~3.25.20"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "~4.4.1",
    "@nuxt/eslint": "1.4.1",
    "@types/jmespath": "~0.15.2",
    "@types/node": "~22.15.21",
    "eslint": "~9.27.0",
    "eslint-config-prettier": "~10.1.5",
    "globals": "~16.1.0",
    "npm-run-all2": "~8.0.3",
    "prettier": "~3.5.3",
    "prettier-plugin-tailwindcss": "~0.6.11",
    "tsx": "~4.19.4",
    "typescript": "~5.8.3",
    "vue-component-type-helpers": "~2.2.10",
    "vue-tsc": "~2.2.10"
  }
}
