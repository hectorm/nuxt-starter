    "@tiptap/extension-table": "~3.6.2",
    "@tiptap/extension-text": "~3.6.2",
    "@tiptap/extension-underline": "~3.6.2",
    "@tiptap/pm": "~3.6.2",
    "@tiptap/vue-3": "~3.6.2",
    "@trpc/client": "~11.6.0",
    "@trpc/server": "~11.6.0",
    "@vueuse/core": "~13.9.0",
    "@vueuse/nuxt": "~13.9.0",
    "@zenstackhq/runtime": "~2.19.2",
    "@zenstackhq/server": "~2.19.2",
    "dompurify": "~3.2.7",
    "github-markdown-css": "~5.8.1",
